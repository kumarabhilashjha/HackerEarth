#include <stdio.h>
#include <string.h>
int main () 
{
   int i=0;
   char ch, s[110];
   gets(s);
   while (s[i]!='\0') {
      if(s[i]>='A' && s[i]<='Z')
         {s[i] = s[i]+32;}
      else if(s[i]>='a' && s[i]<='z')
         {s[i] = s[i]-32;}   
      i++;   
   }
   printf("%s", s);
   return 0;
}
